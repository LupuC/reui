{
  "code": "'use client';\n\nimport Link from 'next/link';\nimport { CircleCheck, Ghost, Star, Users } from 'lucide-react';\nimport { cn } from '@/lib/utils';\nimport { Avatar, AvatarFallback, AvatarImage } from '@/components/ui/avatar';\nimport { Badge } from '@/components/ui/badge';\nimport { Button } from '@/components/ui/button';\nimport { Card, CardContent, CardFooter } from '@/components/ui/card';\n\nconst items = [\n  {\n    icon: <Ghost />,\n    title: 'Pixel Crafters',\n    description: 'Crafting digital experiences for the world',\n    labels: ['Ul', 'DevOps'],\n    connected: true,\n    rating: { value: 3, round: 0 },\n  },\n];\n\nexport default function Block() {\n  return (\n    <div className=\"flex items-center justify-center min-h-screen\">\n      {items.map((item, index) => (\n        <Card key={index} className=\"w-[400px]\">\n          <CardContent>\n            <div className=\"grid place-items-center gap-4\">\n              <div className=\"flex justify-center items-center size-14 rounded-full ring-1 ring-border bg-muted\">\n                {item.icon}\n              </div>\n\n              <div className=\"grid place-items-center\">\n                <Link\n                  href=\"#\"\n                  className=\"text-base font-medium text-foreground hover:text-primary mb-px\"\n                >\n                  {item.title}\n                </Link>\n                <span className=\"text-sm text-mono text-center\">\n                  {item.description}\n                </span>\n              </div>\n            </div>\n\n            <div className=\"grid mt-10\">\n              <div className=\"flex items-center justify-between flex-wrap mb-3.5 gap-2\">\n                <span className=\"text-xs text-subtle uppercase\">skills</span>\n                <div className=\"flex flex-wrap gap-1.5\">\n                  {item.labels.map((label, index) => {\n                    return (\n                      <Badge appearance=\"stroke\" key={index}>\n                        {label}\n                      </Badge>\n                    );\n                  })}\n                </div>\n              </div>\n\n              <div className=\"border-t border-border border-dashed\"></div>\n\n              <div className=\"flex items-center justify-between flex-wrap my-2.5 gap-2\">\n                <span className=\"text-xs text-subtle uppercase\">rating</span>\n                <div className=\"flex\">\n                  {[...Array(5)].map((_, idx) => (\n                    <Star\n                      key={idx}\n                      className={cn(\n                        'size-4 text-secondary',\n                        idx < item.rating.value ? 'text-yellow-500' : '',\n                      )}\n                    />\n                  ))}\n                </div>\n              </div>\n\n              <div className=\"border-t border-border border-dashed mb-3.5\"></div>\n\n              <div className=\"flex items-center justify-between flex-wrap gap-2\">\n                <span className=\"text-xs text-subtle uppercase\">members</span>\n                <div className=\"flex -space-x-3\">\n                  {['2.png', '4.png', '16.png', '8.png'].map((avatar, i) => (\n                    <Avatar key={i} className=\"size-8\">\n                      <AvatarImage\n                        src={`/media/avatars/${avatar}`}\n                        alt=\"@reui\"\n                        className=\"border-2 border-background hover:z-10\"\n                      />\n                      <AvatarFallback>CH</AvatarFallback>\n                    </Avatar>\n                  ))}\n                  <Button\n                    variant=\"secondary\"\n                    mode=\"icon\"\n                    shape=\"circle\"\n                    className=\"relative size-8 border-2 border-background hover:z-10\"\n                  >\n                    +5\n                  </Button>\n                </div>\n              </div>\n  